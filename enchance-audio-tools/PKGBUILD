pkgdesc="Enchance audio for linux who want good experience in music"
arch=("x86_64")
depends=(
    "pipewire"
    "pipewire-alsa"
    "pipewire-pulse"
    "wireplumber"
    "gst-plugin-pipewire"
    "easyeffects"
    "calf"
    "lsp-plugins"
    "zam-plugins-lv2"
    "mda.lv2"
    "yelp"
)
url="https://github.com/crystalforceix/Repo-Host-Tool-Cappa-Linux"
package() {
  mkdir -p "${pkgdir}/usr/"
  cp -r "${srcdir}/usr/bin" "${pkgdir}/usr/"
}

